
<section>
<h1>Burgers Ready to Eat</h1>

<ul>
{{#each burgers}}
    {{#unless devoured}}
      {{> partials devoured=true}}
    {{/unless}}
  {{/each}}
</ul>
</section>

<section>
<h1>Burgers Eaten</h1>

<ul>
{{#each burgers}}
    {{#if devoured}}
      {{> partials devoured=false}}
    {{/if}}
  {{/each}}
</ul>
</section>

<a href="/create"><button id="create" >Create a burger!</button></a>